CREATE TABLE PYTEST.MY_TECH
(
   TECH_ID SERIAL PRIMARY KEY,
   TECHNOLOGY VARCHAR(200) UNIQUE,
   EXPERIANCE INT NOT NULL,
   START_DATE TIMESTAMP(6)
 );
 
 
 SELECT * FROM  PYTEST.MY_TECH;
 
 INSERT INTO PYTEST.MY_TECH (TECHNOLOGY,EXPERIANCE,START_DATE)
 VALUES ('PYSPARK',4,'2021-01-01 00:00:00');

UPDATE PYTEST.MY_TECH 
SET EXPERIANCE = 1
WHERE  TECH_ID = 2
